#!/bin/bash
function getCMD() {
  CMD=""
  SUB=()
  OPT=()
  for i  ; do
    if [ "${CMD}" = "" ]; then
      if [ "${i:0:1}" = "-" ]; then
        OPT+=("${i}")
        continue
      fi
      CMD="${i}"
      continue
    else
      SUB+=("${i}")
    fi
  done
}
getCMD "${@}"

case "${CMD}" in
"contain")
  SRC=""
  for i in "${SUB[@]}" ; do
      if [ "${SRC}" = "" ]; then
        SRC="${i}"
        continue
      fi
      if [ "${SRC}" = "${i}" ]; then
        #echo "true"
          exit 0
      fi
  done
  #echo "false"
  exit 1
  ;;
esac